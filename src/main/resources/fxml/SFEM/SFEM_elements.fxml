<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane focusTraversable="true" prefHeight="800.0" prefWidth="1150.0" styleClass="bkg"
            stylesheets="@../../css/stylesheet.css" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets left="30.0" top="30.0"/>
    </padding>
    <children>
        <VBox alignment="CENTER" layoutX="75.0" layoutY="96.0" prefHeight="560.0" prefWidth="326.0">
            <children>
                <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="titles" text="Modules"
                      textAlignment="CENTER" wrappingWidth="120.138671875">
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </Text>
                <ListView fx:id="SFEMs_list" maxHeight="1.7976931348623157E308" maxWidth="-Infinity"
                          onMouseClicked="#updateElementsTreeView" prefHeight="420.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </ListView>
                <HBox alignment="CENTER" maxWidth="-Infinity" prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="new_sfem" alignment="CENTER" mnemonicParsing="false" onAction="#buttonPressed"
                                prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/Plus_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="edit_sfemName" alignment="CENTER" mnemonicParsing="false"
                                onAction="#buttonPressed" prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/edit_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="delete_sfem" alignment="CENTER" mnemonicParsing="false" onAction="#buttonPressed"
                                prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/minus_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="refresh_sfem" alignment="CENTER" mnemonicParsing="false"
                                onAction="#buttonPressed" prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/refresh_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </HBox>
                <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" prefHeight="40.0" prefWidth="200.0">
                    <children>
                        <Pane prefHeight="40.0" prefWidth="83.0"/>
                        <TextField fx:id="sfem_name" alignment="CENTER" maxWidth="-Infinity" prefHeight="40.0"
                                   prefWidth="217.0" promptText="SFEM name" styleClass="text_field" visible="false"/>
                        <Pane prefHeight="40.0" prefWidth="39.0"/>
                        <Button fx:id="submit_name_btn" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
                                mnemonicParsing="false" onAction="#newSFEM" prefHeight="40.0" prefWidth="55.0"
                                style="-fx-background-color: transparent;" visible="false">
                            <graphic>
                                <ImageView fitHeight="26.0" fitWidth="46.0" pickOnBounds="true" preserveRatio="true"
                                           scaleX="1.5" scaleY="1.5">
                                    <image>
                                        <Image url="@../../icons/right_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox alignment="CENTER" layoutX="433.0" layoutY="96.0" prefHeight="521.0" prefWidth="525.0">
            <children>
                <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="titles"
                      text="SFEM Elements (SFEEs)" textAlignment="CENTER" wrappingWidth="299.138671875">
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </Text>
                <TreeView fx:id="sfem_elements_tree" maxHeight="-Infinity" prefHeight="418.0" prefWidth="525.0">
                    <VBox.margin>
                        <Insets bottom="10.0"/>
                    </VBox.margin>
                </TreeView>
                <HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="40.0"
                      prefWidth="527.0">
                    <children>
                        <Button fx:id="new_sfee" alignment="CENTER" mnemonicParsing="false" onAction="#buttonPressed"
                                prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/Plus_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="edit_sfee" alignment="CENTER" mnemonicParsing="false" onAction="#buttonPressed"
                                prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/edit_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="delete_sfee" alignment="CENTER" mnemonicParsing="false" onAction="#buttonPressed"
                                prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/minus_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Pane prefHeight="40.0" prefWidth="21.0"/>
                        <Button fx:id="refresh_sfee" alignment="CENTER" mnemonicParsing="false"
                                onAction="#buttonPressed" prefHeight="40.0" prefWidth="40.0" textAlignment="CENTER">
                            <graphic>
                                <ImageView fitHeight="30.0" fitWidth="30.0" nodeOrientation="INHERIT"
                                           pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../icons/refresh_64px.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
