<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" styleClass="bkg" stylesheets="@../../css/stylesheet.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="title" alignment="CENTER_RIGHT" layoutX="100.0" layoutY="170.0" prefHeight="47.0" prefWidth="600.0" AnchorPane.leftAnchor="37.0" AnchorPane.topAnchor="10.0">
         <children>
            <Button alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" mnemonicParsing="false" onAction="#goBack" prefHeight="47.0" prefWidth="46.0" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="39.0" fitWidth="43.0" pickOnBounds="true" preserveRatio="true" scaleX="1.5" scaleY="1.5">
                     <image>
                        <Image url="@../../icons/left_64px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Pane prefHeight="47.0" prefWidth="71.0" />
            <TextField fx:id="SFEE_name" prefHeight="40.0" prefWidth="480.0" promptText="Shop Floor Educational Element">
               <styleClass>
                  <String fx:value="transparent" />
                  <String fx:value="titles" />
               </styleClass></TextField>
         </children>
      </HBox>
      <Pane fx:id="SFEE_bar" layoutX="78.0" layoutY="96.0" prefHeight="45.0" prefWidth="1044.0" AnchorPane.leftAnchor="78.0">
         <children>
            <HBox alignment="CENTER" prefHeight="45.0" prefWidth="1044.0">
               <children>
                  <ToggleButton fx:id="toggleProperties" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="250.0" styleClass="sfee-bar-btn" text="Properties">
                     <toggleGroup>
                        <ToggleGroup fx:id="bar" />
                     </toggleGroup></ToggleButton>
                  <Pane focusTraversable="true" prefHeight="45.0" prefWidth="120.0" />
                  <ToggleButton fx:id="toggleCommunication" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="250.0" styleClass="sfee-bar-btn" text="Communication" toggleGroup="$bar" />
                  <Pane prefHeight="200.0" prefWidth="120.0" />
                  <ToggleButton fx:id="toggleItems" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="250.0" styleClass="sfee-bar-btn" text="Items" toggleGroup="$bar" />
                  <Pane prefHeight="200.0" prefWidth="120.0" />
                  <ToggleButton fx:id="toggleFailure" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="250.0" styleClass="sfee-bar-btn" text="Failure" toggleGroup="$bar" />
                  <Pane prefHeight="200.0" prefWidth="120.0" />
                  <ToggleButton fx:id="toggleFinish" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="30.0" prefWidth="250.0" styleClass="sfee-bar-btn" text="Finish" toggleGroup="$bar" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="30.0" right="30.0" />
               </padding>
            </HBox>
         </children></Pane>

      <AnchorPane fx:id="SFEE_body" layoutX="115.0" layoutY="162.0" prefHeight="665.0" prefWidth="1071.0" />
      <Button fx:id="next" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" layoutX="1095.0" layoutY="30.0" mnemonicParsing="false" onAction="#goNext" prefHeight="47.0" prefWidth="46.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="33.0" fitWidth="39.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/right_64px.png" />
               </image>
            </ImageView>
         </graphic>
         <tooltip>
            <Tooltip fx:id="errorMsg" autoHide="true" height="50.0" width="50.0">
               <graphic>
                  <ImageView fx:id="error_icon" fitHeight="26.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true" visible="false">
                     <image>
                        <Image url="@../../icons/cancel_64px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Tooltip>
         </tooltip>
      </Button>
   </children>
   <padding>
      <Insets top="20.0" />
   </padding>
</AnchorPane>
